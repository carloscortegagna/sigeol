<h1>Invito dei docenti</h1>

<% form_tag(:action => :create, :method => :post) do %>
   <p>
    <%= label_tag "Mail" %>
    <%= text_field_tag "mail" %>
  </p>
  <p>
    <%= label_tag 'Corso di laurea' %>
    <%= select_tag "graduate_course_id", options_from_collection_for_select(@graduate_courses, "id", "name") %>
  </p>
  <p>
    <%= submit_tag "Invita docente" %>
  </p>
<% end %>