<h3>Docenti divisi per corso di laurea</h3>
<% @graduate_course.each do |g| %>
  <%  if g.users.size != 0 %>
    <h4> <%= g.name %> </h4>
    <ul>
      <% (g.users).each do |u|%>
        <% if u.specified_type = "Teacher" %>
      <li>  <%= link_to u.specified.surname + " " + u.specified.name, teacher_path(u.specified) %> </li>
       <% end %>
     <% end %>
     <% end %>
    </ul>
<% end %>